/* pages/auth/auth.wxss */

page {
  background-color: #000;
}

.introduction {
  width: 100vw;
  height: calc((667 / 375) * 100vw);
  overflow: hidden;
}

.introduction-x {
  width: 100vw;
  height: calc((812 / 375) * 100vw);
  overflow: hidden;
}

.btn-auth {
  padding: 0;
  outline: none;
  position: fixed;
  object-fit: fill;
  height: calc((57 / 375) * 100vw);
  width: calc((312 / 375) * 100vw);
  background: url(data:image/png;base64,);
}

.btn-normal {
  top: calc((548 / 667) * 100vh);
  left: calc((32 / 375) * 100vw);
  background-size: calc((312 / 375) * 100vw), calc((57 / 375) * 100vw);
  background-repeat: no-repeat;
}

.btn-x {
  top: calc((654 / 812) * 100vh);
  left: calc((32 / 375) * 100vw);
  background-size: calc((312 / 375) * 100vw), calc((57 / 375) * 100vw);
  background-repeat: no-repeat;
}
