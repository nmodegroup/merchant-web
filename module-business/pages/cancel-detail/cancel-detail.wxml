<!--module-business/pages/cancel-detail/cancel-detail.wxml-->
<view class="page">
  <nav-bar title="核销详情" back></nav-bar>
  <view class="group-block">

    <view>
      <view class="group-block__item"
      wx:for="{{ list }}"
      wx:key="{{ index }}"
      >
        <cell-group
         titleLeft="{{ item.code }}"
         twoTipsLeft="{{ item.theme }}"
         twoTipsRight="{{ '核销时间：' + item.date }}"
        />
      </view>
      <!-- <view class="group-block__item">
        <cell-group
        />
      </view> -->
    </view>
    <!-- 加载状态 -->
    <loading-more 
     isShowLoadingMore="{{ isShowLoadingMore }}"
     showNomore="{{ showNomore }}"
     moreTips="{{ moreTips }}" 
    />
  </view>

</view>
